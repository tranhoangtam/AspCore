<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8">
    <meta name="keywords" content="điện thoại di động, dtdd, smartphone, tablet, máy tính bảng, Laptop, máy tính xách tay, phụ kiện điện thoại, tin công nghệ" />
    <meta name="description" content="smartphone, máy tính bảng, tablet, laptop, phụ kiện chính hãng mới nhất, giá tốt, dịch vụ khách hàng được yêu thích nhất VN" />
    <meta property="og:title" content="Tablet, Laptop, Phụ kiện chính hãng" />
    <meta property="og:description" content="smartphone, máy tính bảng, tablet, laptop, phụ kiện chính hãng mới nhất, giá tốt, dịch vụ khách hàng được yêu thích nhất VN" />
    <!-- Favicons Icon -->
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico">
    <title>@ViewBag.Title</title>
    @*Import css*@
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/simple-line-icons.css" rel="stylesheet" />
    <link href="~/css/pe-icon-7-stroke.min.css" rel="stylesheet" />
    <link href="~/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/css/owl.transitions.css" rel="stylesheet" />
    <link href="~/css/flexslider.css" rel="stylesheet" />
    <link href="~/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/css/revolution-slider.css" rel="stylesheet" />
    <link href="~/css/quick_view_popup.css" rel="stylesheet" />
    <link href="~/css/blog.css" rel="stylesheet" />
    <link href="~/css/shortcode.css" rel="stylesheet" />
    <link href="~/css/shortcodes/shortcodes.css" rel="stylesheet" />
    <link href="~/css/shortcodes/featured-box.css" rel="stylesheet" />
    <link href="~/css/shortcodes/pricing-table.css" rel="stylesheet" />
    <link href="~/css/shortcodes/tooltip.css" rel="stylesheet" />
    <link href="~/css/shortcodes/post.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" />
</head>
<body>
    <div id="page" >
        <!-- style="min-height: 800px"-->
        <!-- Header -->
        <header>
            @await Html.PartialAsync("_MyHeader")
        </header>
        <!-- end header -->
        @await Html.PartialAsync("_MyMenu")
        <!--Breadcrumbs-->
        @RenderSection("Breadcrumbs", false)
        <!--/Breadcrumbs-->
        <section class="main-container">
            <div class="container">
                <div class="row">
                    @RenderBody()
                    @RenderSection("MenuLeft", false);
                </div>
            </div>
        </section>
        <!--Footer-->
        @await Html.PartialAsync("_MyFooter")
        <!--/Footer-->
    </div>
    <!--Chèn JS  -->
    <!-- jquery js -->
    <script type="text/javascript" src="js/jquery.min.js"></script>

    <!-- bootstrap js -->
    <script type="text/javascript" src="js/bootstrap.min.js"></script>

    <!-- owl.carousel.min js -->
    <script type="text/javascript" src="js/owl.carousel.min.js"></script>

    <!-- jquery.mobile-menu js -->
    <script type="text/javascript" src="js/mobile-menu.js"></script>

    <!--jquery-ui.min js -->
    <script type="text/javascript" src="js/jquery-ui.js"></script>

    <!-- main js -->
    <script type="text/javascript" src="js/main.js"></script>

    <!-- countdown js -->
    <script type="text/javascript" src="js/countdown.js"></script>

    <!-- Slider Js -->
    <script type="text/javascript" src="js/revolution-slider.js"></script>
</body>
</html>
